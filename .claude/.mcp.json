{
  "mcpServers": {
    "gitlab": {
      "command": "/usr/local/bin/mcp-proxy",
      "args": [
        "--transport",
        "streamablehttp",
        "https://mcp-gitlab.op.barkleyfarm.com/mcp"
      ],
      "description": "GitLab repository and project management via k3s."
    },
    "cloudflare": {
      "command": "/usr/local/bin/mcp-proxy",
      "args": [
        "--transport",
        "streamablehttp",
        "https://mcp-cloudflare.op.barkleyfarm.com/mcp"
      ],
      "description": "Cloudflare infrastructure management via k3s."
    }
  }
}
